{
  "mcpServers": {
    "unity-natural-mcp": {
      "type": "stdio",
      "command": "npm",
      "args": [
        "start",
        "--prefix",
        "../stdio-to-streamable-http/"
      ],
      "env": {
        "MCP_SERVER_IP": "localhost",
        "MCP_SERVER_PORT": "8090"
      }
    }
  }
}